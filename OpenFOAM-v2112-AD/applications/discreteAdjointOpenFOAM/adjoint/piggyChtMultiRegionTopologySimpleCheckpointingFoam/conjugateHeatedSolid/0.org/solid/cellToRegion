FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/solid";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];


internalField   uniform 1;

boundaryField
{
    solid_walls
    {
        type            zeroGradient;
    }
    solid_to_fluid
    {
        type            zeroGradient;
    }
    front_and_back
    {
        type            empty;
    }
}


