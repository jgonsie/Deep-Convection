FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue penalty 0
    volScalarFieldValue alpha 0
);

regions
(
    boxToCell
    {
        box          (-0.011 -1 -0.002)(0.011 1 0.002);
        fieldValues
        (
            volScalarFieldValue penalty 1
            volScalarFieldValue alpha   50000
        );
    }
    boxToCell
    {
        box          (-0.011 -1 -1)(0.011 0 1);
        fieldValues
        (
            volScalarFieldValue penalty 1
            volScalarFieldValue alpha   50000
        );
    }
);


