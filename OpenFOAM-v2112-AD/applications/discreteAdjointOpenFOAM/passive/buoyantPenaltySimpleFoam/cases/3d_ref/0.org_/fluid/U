FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0/domain0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform ( 0.049 0 0 ); // 30l/h
    }
    outlet
    {
        type            inletOutlet;
        value           uniform ( 0 0 0 );
        inletValue      uniform ( 0 0 0 );
    }
    "(fluid_to_solid|walls)"
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
    }
}


