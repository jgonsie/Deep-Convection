Sens
{
  expr "deriv (%f%) %f%";
  name Sens;
  theTitle "Sensitivity";
  titles ( sens );
  type regular;
}

Res
{
  expr "deriv %f% (%f%)";
  name Res;
  theTitle "Residual";
  titles ( residual );
  type regular;
  logscale true;
}

