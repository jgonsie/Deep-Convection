FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 3.75e-3;

boundaryField
{
    upperwall
    {
        type            kqRWallFunction;
        value           uniform 3.75e-3;
    }
	
	lowerwall
    {
        type            kqRWallFunction;
        value           uniform 3.75e-3;
    }
	
	column
    {
        type            kqRWallFunction;
        value           uniform 3.75e-3;
    }
	
    inlet
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity       0.05;
        value           uniform 3.75e-3;
    }
    
	outlet
    {
        type            zeroGradient;
    }
    
	defaultFaces
    {
        type            empty;
    }

}


// ************************************************************************* //
